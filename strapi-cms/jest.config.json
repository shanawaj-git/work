{
  "testPathIgnorePatterns": ["/node_modules/", ".tmp", ".cache"],
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": ["./src/middlewares/**"],
  "coverageThreshold": {
    "global": {
      "lines": 90,
      "statements": 90,
      "branches": 90,
      "functions": 90
    }
  },
  "coverageReporters": ["html", "text", "text-summary", "cobertura"]
}
