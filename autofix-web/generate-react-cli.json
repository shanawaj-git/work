{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": "/__tests__"
    },
    "page": {
      "customTemplates": {
        "component": "templates/pages/index.tsx"
      },

      "path": "src/containers",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": "/__tests__"
    },
    "feature": {
      "customTemplates": {
        "component": "templates/features/TemplateName.ts"
      },
      "path": "src/features",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    },
    "style": {
      "customTemplates": {
        "component": "templates/styles/TemplateName.module.css"
      },
      "path": "src/styles",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    }
  }
}
