{
  "name": "react-template",
  "description": "React Next JS Template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.18.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage",
    "test:ci": "jest --ci",
    "dev:clean": "rm -rf cache && yarn dev",
    "prepare": "husky install",
    "create": "npx @albathanext/react-generate-cli c",
    "check-format": "prettier --check .",
    "check-lint": "yarn lint",
    "check-types": "tsc --pretty --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@albathanext/design-system": "^0.7.5",
    "@apollo/client": "^3.6.9",
    "@reduxjs/toolkit": "^1.8.2",
    "cross-fetch": "^3.1.5",
    "cypress": "^10.3.0",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "^16.5.0",
    "is-ci": "^3.0.1",
    "lodash.get": "^4.4.2",
    "lodash.pickby": "^4.6.0",
    "lodash.range": "^3.2.0",
    "moment": "^2.29.4",
    "next": "12.1.6",
    "next-images": "^1.8.4",
    "next-plugin-svgr": "^1.1.1",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.0.4",
