    "@types/lodash.range": "^3.2.7",
    "@types/node": "18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "autoprefixer": "^10.4.7",
